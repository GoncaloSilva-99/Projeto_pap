<div id="<%= dom_id board_profile %>">
  <p>
    <strong>User:</strong>
    <%= board_profile.user_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= board_profile.name %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= board_profile.bio %>
  </p>

  <p>
    <strong>Birth date:</strong>
    <%= board_profile.birth_date %>
  </p>

  <p>
    <strong>Club profile:</strong>
    <%= board_profile.club_profile_id %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= board_profile.role %>
  </p>

</div>
