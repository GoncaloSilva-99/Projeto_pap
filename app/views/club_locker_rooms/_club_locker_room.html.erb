<div id="<%= dom_id club_locker_room %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Club profile:</strong>
    <%= club_locker_room.club_profile_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Club training center:</strong>
    <%= club_locker_room.club_training_center_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sport:</strong>
    <%= club_locker_room.sport_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= club_locker_room.name %>
  </div>
</div>
