<div id="<%= dom_id club_team_training %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Club locker room:</strong>
    <%= club_team_training.club_locker_room_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Club pitch:</strong>
    <%= club_team_training.club_pitch_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Start time:</strong>
    <%= club_team_training.start_time %>
  </div>
  <div>
    <strong class="block font-medium mb-1">End time:</strong>
    <%= club_team_training.end_time %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Recurring:</strong>
    <%= club_team_training.recurring? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Weekday:</strong>
    <%= club_team_training.weekday %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Club team:</strong>
    <%= club_team_training.club_team_id %>
  </div>
</div>
